<kmodule xmlns="http://www.drools.org/xsd/kmodule" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <kbase name="alert-stateful" default="true" eventProcessingMode="stream" equalsBehavior="identity">
    <ksession name="alertStateful" type="stateful" default="true" clockType="realtime"/>
  </kbase>
  <kbase name="alert-stateless" default="false" eventProcessingMode="cloud" equalsBehavior="identity">
    <ksession name="alertStateless" type="stateless" default="true" clockType="realtime"/>
  </kbase>
</kmodule>